{
    "name": "michaelthuren/tinify-laravel-client",
    "type": "library",
    "description": "Tinify API support with laravel (TinyPNG)",
    "keywords": [
        "tinify",
        "tinypng",
        "laravel",
        "client"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Thuren",
            "email": "michael.thuren@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "tinify/tinify": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "MichaelThuren\\TinifyLaravel\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelThuren\\TinifyLaravel\\TinifyLaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
